{
    "name": "Process web helper",
    "version": "1.5",
    "manifest_version": 3,
    "description": "Block requests.",
    "background": {
        "service_worker": "service_worker.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "externally_connectable": {
        "matches": ["<all_urls>"]
    },
    "permissions": [
        "declarativeNetRequest",
        "sidePanel",
        "webRequest",
        "tabs",
        "storage"
    ],
    "action": {
        "default_title": "Click here!"
    },
    "side_panel": {
        "default_path": "sidepanel/sidepanel.html"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B"
            }
        }
    },
    "icons": {
        "16": "images/16.png",
        "48": "images/48.png",
        "128": "images/128.png"
    }
}
